{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ true ],
        "only-arrow-functions": [false],
        "no-empty": [false],
        "no-console": [false],
        "quotemark": [false],
        "object-literal-sort-keys": [false],
        "ordered-imports": [false],
        "no-debugger": [false],
        "no-trailing-whitespace": [false],
        "no-var-requires": [false],
        "max-classes-per-file": [false],
        "no-string-literal": [false]
    },
    "rulesDirectory": []

}